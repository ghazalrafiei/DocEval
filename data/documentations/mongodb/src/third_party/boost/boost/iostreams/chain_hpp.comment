['text':'        bool is_complete() const;                  // Ready for i/o.','line_number':89,'multiline':False]
['text':'        template<typename T>','line_number':90,'multiline':False]
['text':'        void push( const T& t,                     // Adds a stream buffer to','line_number':91,'multiline':False]
['text':'                   streamsize,                     // chain, based on t, with','line_number':92,'multiline':False]
['text':'                   streamsize );                   // given buffer and putback','line_number':93,'multiline':False]
