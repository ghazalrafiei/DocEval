['text':'                         3             max count   (-1 for unbounded)','line_number':2450,'multiline':False]
['text':'                         4  ...        block to be iterated over','line_number':2451,'multiline':False]
['text':'                         5  CTR_LOOP','line_number':2452,'multiline':False]
['text':'','line_number':2453,'multiline':False]
['text':'                       In','line_number':2454,'multiline':False]
['text':'------------------------------------------------------------------------------','line_number':2455,'multiline':False]
